
    <style>
        .schedule-table{
            position: relative;
            padding-left: 50px;
        }
        .times{
            position: absolute;
            left: 0;
        }
        .times .item{
            font-size: 14px;
            font-weight: 700;
            color: #646464;
            height: 80px;
            padding-top: 10px;
        }
    </style>

    <div class="schedule-table">
        <table>

            <style>
                .schedule-table .title{
                    font-size: 16px;
                    font-weight: 700;
                    text-align: center;
                    color: #646464;
                    margin: 0 0 10px;
                }
            </style>

            <tr>
                {{#events_halls}}
                    <td>
                        <div class="title">
                            {{hall_name}}
                        </div>
                    </td>
                {{/events_halls}}
            </tr>
            <tr>
                <td>
                    <ul class="times">
                        <li class="item">10:00</li>
                        <li class="item">11:00</li>
                        <li class="item">12:00</li>
                        <li class="item">13:00</li>
                        <li class="item">14:00</li>
                        <li class="item">15:00</li>
                        <li class="item">16:00</li>
                        <li class="item">17:00</li>
                        <li class="item">18:00</li>
                    </ul>
                </td>
            </tr>
            <tr>
                {{#events_halls}}
                <td>


                    {{#events}}
                        <div class="data-block">
                            <div class="time-interval">
                                {{#time}}
                                    {{start_time_str}} –
                                    {{end_time_str}}
                                {{/time}}
                            </div>
                            <div class="name">
                                <p>
                                    {{#name}}
                                        {{#ru}}
                                            {{str}}
                                        {{/ru}}
                                    {{/name}}
                                </p>
                            </div>
                            <div class="themes">
                                {{#themes}}
                                    {{#strings}}
                                        {{#ru}}
                                            {{str}}
                                        {{/ru}}
                                    {{/strings}}
                                {{/themes}}
                            </div>
                        </div>
                    {{/events}}
                </td>
            {{/events_halls}}

