
    <style>
        .schedule-table{
            height: 950px;
            position: relative;
            padding-left: 50px;
        }
        .times{
            position: absolute;
            left: 0;
        }
        .times .item{
            font-size: 14px;
            font-weight: 700;
            color: #646464;
            height: 100px;
            padding-top: 10px;
        }

        .schedule-table .title{
            font-size: 16px;
            font-weight: 700;
            text-align: center;
            color: #646464;
            margin: 0 0 10px;
        }
        .data-block .themes{
            position: absolute;
            width: 100%;
            padding: 0 5px;
            box-sizing: padding-box;
            bottom: 5px;
            right: 0;
            text-align: right;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
        .data-block .name{
            overflow: hidden;
            position: absolute;
            box-sizing: border-box;
            border-bottom: solid 38px rgba(0,0,0,0);
            border-top: solid 20px rgba(0,0,0,0);
            height: 100%;
        }
        .col{
            vertical-align: top;
            height: 950px;
        }
        .dots{
            background: url('data:image/png;base64,');
            background-repeat: repeat-x;
            background-position: 0 13px;
        }
    </style>

    <div class="schedule-table">
        <table class="maintable">

            <tr>
                {{#events_halls}}
                    <td>
                        <div class="title" style="max-width: 250px; margin-left: 15px; margin-right: 15px;">
                            {{hall_name}}
                        </div>
                    </td>
                {{/events_halls}}

                <td>
                    <div class="title" style="margin-left: 15px; margin-right: 15px; text-align: center;">
                        Презентации на стендах
                    </div>
                </td>

            </tr>
            <tr>
                <td>
                    <ul class="times" style="border-right: solid 1px #e1e1e1;">
                        <li class="item">10:00</li>
                        <li class="item">11:00</li>
                        <li class="item">12:00</li>
                        <li class="item">13:00</li>
                        <li class="item">14:00</li>
                        <li class="item">15:00</li>
                        <li class="item">16:00</li>
                        <li class="item">17:00</li>
                        <li class="item">18:00</li>
                    </ul>
                </td>
            </tr>
            <tr>
                {{#events_halls}}
                    <td class="col dots">


                        {{#events}}

                            <div style="position: relative;">
                                <div style="position: absolute; width: 100%; height: 900px;  border-right: solid 1px #e1e1e1;">
                                    <div class="data-block"  style="text-overflow: ellipsis; margin: 0 15px; padding: 5px; position: relative; top: {{#time}}{{top}}{{/time}}px; height: {{#time}}{{height}}{{/time}}px;">
                                        <div class="time-interval" style="position: absolute;">
                                            {{#time}}
                                                {{start_time_str}} –
                                                {{end_time_str}}
                                            {{/time}}
                                        </div>
                                        <div class="name">
                                            <p style="margin-bottom: 10px;">
                                                {{#name}}
                                                    {{#ru}}
                                                        {{str}}
                                                    {{/ru}}
                                                {{/name}}
                                            </p>
                                            <p style="color: #999;">
                                                {{#description}}
                                                    {{#ru}}
                                                        {{{str}}}
                                                    {{/ru}}
                                                {{/description}}
                                            </p>
                                        </div>
                                        <div class="themes">
                                            {{#themes}}
                                                {{#strings}}
                                                    {{#ru}}
                                                        {{str}}
                                                    {{/ru}}
                                                {{/strings}}
                                            {{/themes}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        {{/events}}
                    </td>
                {{/events_halls}}

                    <td style="position: relative; width: 100%; vertical-align: top;">

                        <div style="position: absolute; width: 100%; height: 950px; overflow: auto;">
                            <table style="table-layout: fixed;">
                                <tr>
                                {{#events_booths}}
                                    <td class="dots" style="vertical-align: top; height: 920px;">
                                        {{#.}}

                                        <div style="position: relative;width: 200px;">
                                            <div style="position: absolute; width: 100%; height: 800px;">
                                                <div class="data-block"  style="margin: 0 15px; padding: 5px; position: relative; top: {{#time}}{{top}}{{/time}}px; height: {{#time}}{{height}}{{/time}}px;">
                                                    <div class="time-interval">
                                                        {{#time}}
                                                        {{start_time_str}} –
                                                        {{end_time_str}}
                                                        {{/time}}
                                                    </div>
                                                    <div class="name">
                                                        <p style="margin-bottom: 10px;">
                                                            {{#name}}
                                                            {{#ru}}
                                                            {{str}}
                                                            {{/ru}}
                                                            {{/name}}
                                                        </p>
                                                        <p style="color: #999;">
                                                            {{#description}}
                                                            {{#ru}}
                                                            {{{str}}}
                                                            {{/ru}}
                                                            {{/description}}
                                                        </p>
                                                    </div>
                                                    <div class="themes">
                                                        {{#themes}}
                                                        {{#strings}}
                                                        {{#ru}}
                                                        {{str}}
                                                        {{/ru}}
                                                        {{/strings}}
                                                        {{/themes}}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        {{/.}}
                                    </td>
                                {{/events_booths}}
                                </tr>
                            </table>
                        </div>

                    </td>

