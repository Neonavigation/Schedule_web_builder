<div class="schedule-tabs">
    <div class="schedule-tabs-wrap">
        <ul class="items">
            {{#halls_splits}}
                <li class="item enabled {{#selected}}selected{{/selected}}" data-hall_fr_names='{{hall_fr_names}}' test='{{selected}}'>
                    <a href="#" title="{{#hall_fr_names}}{{.}}&#013;{{/hall_fr_names}}">
                        {{#hall_fr_names}}
                            <span class="name nobr">{{.}}</span>
                        {{/hall_fr_names}}
                    </a>
                </li>
            {{/halls_splits}}
        </ul>
    </div>
</div>


<div class="schedule-table">
    <table class="maintable">
        <colgroup>
            <col class="width-column width{{events_halls.length}} js-width-control">
            <col>
        </colgroup>
        <tr class="table-headers">
            <td class="block-halls">
                <table class="datatable">
                    <tr>
                        {{#events_halls}}
                        <td class="cell">
                            <div class="line js-bg-halls bg-halls"></div>
                            <div class="title">
                                {{hall_name}}
                            </div>
                        </td>
                        {{/events_halls}}
                    </tr>
                </table>
            </td>
            <td class="block-stands bg-gray js-bg-stands bg-stands">
                <table class="datatable">
                    <tr>
                        <td class="cell">
                            <div class="title">
                                Презентации на стендах
                            </div>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>

        <tr class="table-times">
            <td colspan="2">
                <ul class="times">
                    {{#timeline}}
                    <li class="item">{{.}}:00</li>
                    {{/timeline}}
                </ul>
            </td>
        </tr>

        <tr class="table-contents">
            <td class="table-contents-halls height{{timeline.length}}">

                <div class="control">
                    <i class="fa fa-arrow-circle-o-right js-halls-control halls-control"></i>
                </div>

                <table class="datatable">
                    <tr>
                        {{#events_halls}}
                        <td class="halls height{{timeline.length}}">

                            <div class="line js-bg-halls bg-halls"></div>

                            {{#events}}

                            <div class="col-outer">
                                <div class="col-inner">

                                    <div class="data-block js-data-block {{#themes.length}}with-themes{{/themes.length}}" title="{{#name}}{{#ru}}{{str}}{{/ru}}{{/name}}" style="top: {{#time}}{{top}}{{/time}}px; height: {{#time}}{{height}}{{/time}}px;" data-id="{{_id}}">
                                        <div class="data-wrap">
                                            <div class="time-interval">
                                                {{#time}}
                                                {{start_time_str}} –
                                                {{end_time_str}}
                                                {{/time}}
                                            </div>
                                            <div class="text">
                                                <p>
                                                    {{#name}}
                                                    {{#ru}}
                                                    {{str}}
                                                    {{/ru}}
                                                    {{/name}}
                                                </p>
                                                <p class="description">
                                                    {{#description}}
                                                    {{#ru}}
                                                    {{{str}}}
                                                    {{/ru}}
                                                    {{/description}}
                                                </p>
                                            </div>
                                            <div class="themes">
                                                {{#themes}}
                                                {{#strings}}
                                                {{#ru}}
                                                {{str}}
                                                {{/ru}}
                                                {{/strings}}
                                                {{/themes}}
                                            </div>
                                            <div class="icons">
                                                <i class="fa fa-star"></i>
                                                <i class="fa fa-thumbs-o-up"></i>
                                            </div>
                                            <div class="top-gradient"></div>
                                            <div class="bottom-gradient"></div>
                                        </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            {{/events}}
                        </td>
                        {{/events_halls}}
                    </tr>
                </table>

            </td>
            <td class="table-contents-stands stands-block bg-gray js-bg-stands bg-stands height{{timeline.length}}">

                <div class="control">
                    <i class="fa fa-arrow-circle-left js-stands-control stands-control"></i>
                </div>

                <table class="datatable height{{timeline.length}}">
                    <tr>
                        <td class="stands">
                            <div class="stands-container">
                                <table class="stands-table">
                                    <tr>
                                        {{#events_booths}}
                                        <td class="cell height{{timeline.length}}">
                                            {{#.}}

                                            <div class="col-outer">
                                                <div class="col-inner">

                                                    <div class="data-block js-data-block {{#themes.length}}with-themes{{/themes.length}}" title="{{#name}}{{#ru}}{{str}}{{/ru}}{{/name}}" style="top: {{#time}}{{top}}{{/time}}px; height: {{#time}}{{height}}{{/time}}px;" data-id="{{_id}}">
                                                        <div class="data-wrap">
                                                            <div class="time-interval">
                                                                {{#time}}
                                                                {{start_time_str}} –
                                                                {{end_time_str}}
                                                                {{/time}}
                                                            </div>
                                                            <div class="text">
                                                                <p>
                                                                    {{#name}}
                                                                    {{#ru}}
                                                                    {{str}}
                                                                    {{/ru}}
                                                                    {{/name}}
                                                                </p>
                                                                <p class="description">
                                                                    {{#description}}
                                                                    {{#ru}}
                                                                    {{{str}}}
                                                                    {{/ru}}
                                                                    {{/description}}
                                                                </p>
                                                            </div>
                                                            <div class="themes">
                                                                {{#themes}}
                                                                {{#strings}}
                                                                {{#ru}}
                                                                {{str}}
                                                                {{/ru}}
                                                                {{/strings}}
                                                                {{/themes}}
                                                            </div>
                                                            <div class="icons">
                                                                <i class="fa fa-star"></i>
                                                                <i class="fa fa-thumbs-o-up"></i>
                                                            </div>
                                                            <div class="top-gradient"></div>
                                                            <div class="bottom-gradient"></div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>

                                            {{/.}}
                                        </td>
                                        {{/events_booths}}
                                    </tr>
                                </table>
                            </div>
                        </td>
                    </tr>
                </table>

            </td>
        </tr>
    </table>
</div>

<div class="schedule-tabs">
    <div class="schedule-tabs-wrap">
        <ul class="items">
            {{#halls_splits}}
                <li class="item enabled {{#selected}}selected{{/selected}}">
                    <a href="#" title="{{#hall_fr_names}}{{.}}&#013;{{/hall_fr_names}}">
                        {{#hall_fr_names}}
                            <span class="name nobr">{{.}}</span>
                        {{/hall_fr_names}}
                    </a>
                </li>
            {{/halls_splits}}
        </ul>
    </div>
</div>