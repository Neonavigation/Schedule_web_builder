

    <div class="schedule-table">
        <table class="maintable">
            <tr>
                {{#events_halls}}
                <td>
                    <div class="title col1">
                        {{hall_name}}
                    </div>
                </td>
                {{/events_halls}}

                <td>
                    <div class="title col2">
                        Презентации на стендах
                    </div>
                </td>

            </tr>
            <tr>
                <td colspan="2">
                    <ul class="times">
                        <li class="item">10:00</li>
                        <li class="item">11:00</li>
                        <li class="item">12:00</li>
                        <li class="item">13:00</li>
                        <li class="item">14:00</li>
                        <li class="item">15:00</li>
                        <li class="item">16:00</li>
                        <li class="item">17:00</li>
                        <li class="item">18:00</li>
                    </ul>
                </td>
            </tr>
            <tr>

                {{#events_halls}}
                <td class="halls">

                    {{#events}}

                        <div style="position: relative;">
                            <div style="position: absolute; width: 100%; height: 900px;">
                                <div class="data-block"  style="top: {{#time}}{{top}}{{/time}}px; height: {{#time}}{{height}}{{/time}}px;">
                                    <div class="data-wrap">
                                        <div class="time-interval">
                                            {{#time}}
                                                {{start_time_str}} –
                                                {{end_time_str}}
                                            {{/time}}
                                        </div>
                                        <div class="text">
                                            <p style="margin-bottom: 10px;">
                                                {{#name}}
                                                    {{#ru}}
                                                        {{str}}
                                                    {{/ru}}
                                                {{/name}}
                                            </p>
                                            <p style="color: #999;">
                                                {{#description}}
                                                    {{#ru}}
                                                        {{{str}}}
                                                    {{/ru}}
                                                {{/description}}
                                            </p>
                                        </div>
                                        <div class="themes">
                                            {{#themes}}
                                                {{#strings}}
                                                    {{#ru}}
                                                        {{str}}
                                                    {{/ru}}
                                                {{/strings}}
                                            {{/themes}}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    {{/events}}
                </td>
                {{/events_halls}}

                <td class="stands">

                    <div style="position: absolute; width: 99%; height: 960px; overflow: auto;">
                        <table style="table-layout: fixed;">
                            <tr>
                            {{#events_booths}}
                                <td style="vertical-align: top; height: 900px;">
                                    {{#.}}

                                    <div style="position: relative;width: 200px;">
                                        <div style="position: absolute; width: 100%; height: 800px;">
                                            <div class="data-block"  style="top: {{#time}}{{top}}{{/time}}px; height: {{#time}}{{height}}{{/time}}px;">
                                                <div class="data-wrap">
                                                    <div class="time-interval">
                                                        {{#time}}
                                                        {{start_time_str}} –
                                                        {{end_time_str}}
                                                        {{/time}}
                                                    </div>
                                                    <div class="text">
                                                        <p style="margin-bottom: 10px;">
                                                            {{#name}}
                                                            {{#ru}}
                                                            {{str}}
                                                            {{/ru}}
                                                            {{/name}}
                                                        </p>
                                                        <p style="color: #999;">
                                                            {{#description}}
                                                            {{#ru}}
                                                            {{{str}}}
                                                            {{/ru}}
                                                            {{/description}}
                                                        </p>
                                                    </div>
                                                    <div class="themes">
                                                        {{#themes}}
                                                        {{#strings}}
                                                        {{#ru}}
                                                        {{str}}
                                                        {{/ru}}
                                                        {{/strings}}
                                                        {{/themes}}
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    {{/.}}
                                </td>
                            {{/events_booths}}
                            </tr>
                        </table>
                    </div>

                </td>
            </tr>
        </table>

    </div>